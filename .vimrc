syntax on
filetype plugin indent on

set autoindent
set tabstop=4
set shiftwidth=4
set noexpandtab
set ruler
set backspace=indent,eol,start
set mouse=a
set lazyredraw
set t_vb=""
set noundofile
set nobackup

colorscheme base16-atelier-dune

highlight LineNr ctermfg=yellow
highlight LineNr ctermbg=0
highlight Comment ctermfg=grey

